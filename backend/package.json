{
  "name": "dheenotifications-backend",
  "version": "2.0.0",
  "description": "Enterprise Notification Platform Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/worker/notificationWorker.ts",
    "build": "tsc && npx prisma generate",
    "start": "node dist/index.js",
    "start:worker": "node dist/worker/notificationWorker.js",
    "migrate": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "generate": "npx prisma generate",
    "postinstall": "npx prisma generate",
    "studio": "npx prisma studio"
  },
  "keywords": ["notifications", "enterprise", "api", "typescript"],
  "author": "Dheemanth M",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.53.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.1.0",
    "nodemailer": "^7.0.3",
    "prisma": "^6.9.0",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.15.30",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.1",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}